{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "detector (sift)",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/external/PointFeatureHub2/run.py",
            "cwd": "${workspaceFolder}/external/PointFeatureHub2/",
            "args": [
                "task=detect",
                "detect=sift",
                "draw_keypoints=false"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "detector (sp)",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/external/PointFeatureHub2/run.py",
            "cwd": "${workspaceFolder}/external/PointFeatureHub2/",
            "args": [
                "task=detect",
                "detect=super_point",
                "draw_keypoints=false",
                "save_image=true"
            ],
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "detector (r2d2)",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/external/PointFeatureHub2/run.py",
            "cwd": "${workspaceFolder}/external/PointFeatureHub2/",
            "args": [
                "task=detect",
                "detect=r2d2",
                "draw_keypoints=false"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "check sequence (ev)",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/external/Easy3DViewer/example/python/check_sequence_poses.py",
            "args": [
                "--traj_path=${workspaceFolder}/log/poses",
                "--data_path=${workspaceFolder}/data/cracker_box_1",
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "r2d2 (server)",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/r2d2/run_server.py",
            "args": [
                "--port=5556",
                "--model=models/r2d2_WASF_N16.pt",
                "--top-k=500"
            ],
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceFolder}/r2d2"
        },
        {
            "name": "bundle_track_ycb",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/bundle_track_ycbineoat",
            "args": [
                "${workspaceFolder}/config_ycbineoat.yml",
            ],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "bundle_track_colmap",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/bundle_track_colmap",
            "args": [
                "${workspaceFolder}/config_colmap.yml",
            ],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
    ]
}